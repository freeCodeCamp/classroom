{
  "name": "@freecodecamp/classroom",
  "version": "0.0.0",
  "private": true,
  "description": "Classroom mode for freeCodeCamp",
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freecodecamp/classroom.git"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/freecodecamp/classroom/issues"
  },
  "homepage": "https://github.com/freecodecamp/classroom#readme",
  "scripts": {
    "develop": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npm-run-all lint:*",
    "lint:code": "eslint . --ext .js --cache",
    "lint:pretty": "prettier --ignore-path .gitignore --check  .",
    "format": "prettier --ignore-path .gitignore --write .",
    "prepare": "husky install",
    "create-mock-user-server": "npx json-server --watch mock-json-server/studentdata.json",
    "create-mock-authentication-server": "npx json-server --watch mock-json-server/studentprofile.json"
  },
  "dependencies": {
    "@headlessui/react": "1.6.1",
    "@next-auth/prisma-adapter": "1.0.3",
    "@prisma/client": "3.13.0",
    "flowbite": "1.4.5",
    "json-server": "0.17.0",
    "next": "12.1.6",
    "next-auth": "4.3.4",
    "react": "17.0.2",
    "react-copy-to-clipboard": "5.1.0",
    "react-data-table-component": "7.5.1",
    "react-dom": "17.0.2",
    "react-tabs": "4.2.1",
    "styled-components": "5.3.5",
    "react-multi-select-component": "4.2.5",
    "react-select": "5.3.2"
  },
  "devDependencies": {
    "autoprefixer": "10.4.7",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-utils": "3.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.4.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.13",
    "prettier": "2.6.2",
    "prisma": "3.13.0",
    "tailwindcss": "3.0.24"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
